File[] makabho=ja.listFiles();
		
		File[] allFiles = ja.listFiles(new FilenameFilter() {
		    public boolean accept(File dir, String name) {
		        return (name.endsWith(".jpg") || name.endsWith(".jpeg") || name.endsWith(".png"));
		    }
		});
